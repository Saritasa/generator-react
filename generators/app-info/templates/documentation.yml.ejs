toc:
  - name: "<%= moduleName.split('/').join('::') %>"
    description: |
      Methods, actions, sagas, schema and types to work with <%= Name %>.
    children:
    - <%= moduleName %>
    - <%= moduleName %>/inject
    - <%= moduleName %>/actions
    - <%= moduleName %>/actionTypes
    - <%= moduleName %>/api
    - <%= moduleName %>/reducer
    - <%= moduleName %>/sagas
    - <%= moduleName %>/selectors
    - <%= moduleName %>/utils
